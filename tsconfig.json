{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "paths": {
      "@Shipping/*": ["./*"],

      "@Models/*": ["./src/Dtos/*"],
      "@Repositories/*": ["./src/Repositories/*"],
      "@Services/*": ["./src/services/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
